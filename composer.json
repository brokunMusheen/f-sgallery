{
    "name": "brokunMusheen/f-sgallery",
    "description": "Filesystem-based gallery",
    "authors": [
        {
            "name": "Cody",
            "email": "codymgibbs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "symfony/finder": "2.5.*",
        "symfony/filesystem": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "BrokunMusheen\\FSGallery": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "mikey179/vfsStream": "^1.6"
    }
}
